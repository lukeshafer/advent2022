{
	"compilerOptions": {
		"module": "ES2022",
		"allowJs": false,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"noImplicitAny": true,
		"outDir": "build",
		"target": "es2017",
		"types": ["bun-types"]
	}
}
